// Fibonacci no useing Recursion in cpp


#include <iostream>

using namespace std;

int fibo(int n)
{
    if(n<2) {
        return 1;
    }

    return fibo(n-2) + fibo(n-1);
}

int main()
{
    int a;
    cout << "enter any no:" << endl;
    cin >> a;
    cout << "your fiboncii no is:" <<fibo(a) << endl;

    return 0;
}
